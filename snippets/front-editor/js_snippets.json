{
  "@prisma-cms/front-editor Component": {
    "prefix": "@prisma-cms/front-editor Component",
    "description": "Create Component",
    "body": [
      "import React from 'react';\n",
      "import EditorComponent from '@prisma-cms/front-editor/lib/components/App/components/';\n",
      "export class ${1:${TM_DIRECTORY/^.+\\/(.*)$/$1/}} extends EditorComponent {\n",
      "\tstatic Name = '${1}';\n",
      "\tstatic defaultProps = {",
      "\t\t...EditorComponent.defaultProps,",
      "\t}\n\n",
      "\trenderPanelView() {\n",
      "\t\tconst {",
      "\t\t\tclasses,",
      "\t\t} = this.getEditorContext();\n",
      "\t\treturn super.renderPanelView(",
      "\t\t\t<div",
      "\t\t\t\tclassName={classes.panelButton}",
      "\t\t\t>",
      "\t\t\t\t${1}",
      "\t\t\t</div>",
      "\t\t);",
      "\t}\n\n",
      "\tgetRootElement() {",
      "",
      "\t\treturn super.getRootElement();",
      "\t}\n\n",
      "\tcanBeParent(parent) {",
      "",
      "\t\treturn super.canBeParent(parent);",
      "\t}",
      "",
      "",
      "\tcanBeChild(child) {",
      "",
      "\t\treturn super.canBeChild(child);",
      "\t}",
      "",
      "",
      "\trenderChildren() {",
      "",
      "\t\tconst {",
      "\t\t} = this.context;",
      "",
      "\t\tconst {",
      "\t\t} = this.getEditorContext();",
      "",
      "\t\tconst {",
      "\t\t\t...other",
      "\t\t} = this.getComponentProps(this);",
      "",
      "\t\treturn super.renderChildren();",
      "\t}\n",
      "}\n",
      "export default ${1};"
    ]
  },
  "Output": {
    "prefix": "output",
    "description": "Dbms output",
    "body": [
      "dbms_output.put_line('');"
    ]
  }
}